{
  "expo": {
    "name": "Star Wars Trivia",
    "slug": "star-wars-trivia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F0F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0F0F"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Star Wars Trivia App powered by GraphQL, Apollo, and React Native. Written in TypeScript.",
    "githubUrl": "https://github.com/akshaypatel99/star-wars-gql",
    "primaryColor": "#0F0F0F"
  }
}
